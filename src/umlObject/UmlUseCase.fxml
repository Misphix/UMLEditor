<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.Ellipse?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>

<fx:root type="javafx.scene.layout.Pane" xmlns="http://javafx.com/javafx/8.0.60"
	stylesheets="@UmlShape.css" xmlns:fx="http://javafx.com/fxml">
	<StackPane prefHeight="50.0" prefWidth="100.0">
		<children>
			<Ellipse fill="DODGERBLUE" radiusX="60.0" radiusY="30.0"
				stroke="BLACK" strokeType="INSIDE" />
			<Text fx:id="useCaseName" styleClass="use-case-text" strokeType="OUTSIDE" strokeWidth="0.0" text="Use Case" />
		</children>
	</StackPane>
</fx:root>

